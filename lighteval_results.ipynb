      "source": [
        "Install the `lighteval` package."
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "id": "2231e13b-8048-40eb-8691-c3aa39058add",
      "metadata": {
        "id": "2231e13b-8048-40eb-8691-c3aa39058add"
      },
